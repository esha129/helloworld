N = int(input("Enter a number."))
fact = 1
print (range(1,N))
print(range(1, N+1))
for i in range(1,N+1):
  fact = fact*i
print("N! is: ", fact)
# the product of the number and all the numbers before it. eg. factorial of 5, which is shown as '5!' is 1*2*3*4*5


print(range(1, 10))
print(list(range(1,11)))

for i in range(1, 101):
  if i%7 == 0 and i%3 != 0:
    print(i)
    #printing all numbers b/w 1 to 100 that are completely divisible by 7 but not divisible by 3

for i in range(1, 101):
  if i%7 == 0:
    print(i)
    #prints all numbers b/w 1-100 that are completely divisible by 7

for i in range (7, 101, 7):
  print(i)
  # printing all numbers between 7 to 100 in increments of 7

n = int(input("Enter a number: "))
num_multiples = 0
#it is assigning the variable 'num_multiples' the value of 0
for i in range(2,n):
  if n%i==0:
    num_multiples = 1
    # since it is indented, this variable will be increasing with increments of 1
    print("Not a prime number :(")
    break
if num_multiples == 0:
  #if the number of multiples
    print("It is a prime number:)")


n = int(input("enter a number"))
num_multiples = 0
i = 2

while num_multiples == 0 and i<n:
  if n%i==0:
    num_multiples = 1
  i = i + 1

if num_multiples == 0:
  print("prime")
else:
  print ("not prime")
